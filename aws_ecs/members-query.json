{
  "containerDefinitions": [
    {
      "name": "MembersQuery",
      "image": "ukpds/members-query",
      "cpu": 10,
      "memory": 256,
      "essential": true,
      "command": [],
      "portMappings": [
        {
          "hostPort": 83,
          "containerPort": 3000,
          "protocol": "tcp"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "MembersQuery",
          "awslogs-region": "eu-west-1"
        }
      }
    }
  ],
  "family": "MembersQuery"
}